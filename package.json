{
  "name": "vmlist",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon ./bin/www",
    "both": "concurrently --kill-others-on-fail \"npm run client\" \"npm run server\"",
    "start": "node ./bin/www",
    "dokku": {
      "postdeploy": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
    }
  },
  "dependencies": {
    "aws-sdk": "^2.348.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "mongoose": "^5.3.9",
    "morgan": "~1.9.0",
    "vsphere-connect": "^1.1.2"
  }
}