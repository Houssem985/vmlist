{
  "name": "vmlist",
  "version": "0.0.0",
  "private": true,
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ]
  },
  "engines": {
    "node": "10.15.x",
    "npm": "6.4.x"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon ./bin/www",
    "both": "concurrently --kill-others-on-fail \"npm run client\" \"npm run server\"",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "aws-sdk": "^2.506.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^7.0.0",
    "express": "~4.16.0",
    "jsonwebtoken": "^8.5.1",
    "mongo-express": "^0.51.2",
    "mongoose": "5.7.5",
    "mongoose-find-or-create": "^1.3.1",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-google-token": "^0.1.2",
    "passport-jwt": "^4.0.0",
    "vsphere-connect": "^1.1.2"
  }
}
